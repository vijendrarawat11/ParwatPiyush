@model List<ParwatPiyushNewsPortal.Models.Topics>

@foreach (var topic in Model)
{
    <li class="nav-item">
        <a class="nav-link text-dark"
           asp-controller="Home"
           asp-action="Topic"
           asp-route-name="@topic.Name">
            @topic.Name
        </a>
    </li>
}
